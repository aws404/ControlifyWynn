      "com.github.aws404.controlifywynn.ControlifyWynnMod"
    ],
    "controlify": [
      "com.github.aws404.controlifywynn.WynnControlifyEntrypoint"
    ]
  },
  "mixins": [
    "controlifywynn.client.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": "1.20.2",
