  "authors": [
    "aws404"
  ],
  "contact": {
    "sources": "https://github.com/aws404/ControlifyWynn"
  },
  "license": "CC0-1.0",
  "icon": "assets/controlifywynn/icon.png",
  "environment": "client",
  "entrypoints": {
